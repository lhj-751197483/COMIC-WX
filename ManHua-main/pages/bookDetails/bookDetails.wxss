.navbar{
    display: flex;
    height: 100%;
    justify-items: center;
    align-items: center;
    color:rgb(140, 140, 140);
}

.comic-wrapper {
  padding: 0 var(--padding-md, 38rpx);
  margin-bottom: 200rpx;
}

.comic-wrapper .comic-details {
  display: flex;
  padding-left: 20rpx;
}

.comic-wrapper .comic-details .cover {
  margin-right: 40rpx;
  width: var(--comic-cover_3_4-width, 31.5vw);
  height: var( --comic-cover_3_4-height, calc(31.5vw * 4 / 3));
  border-radius: var(--comic-cover_3_4-boder-radius, 10rpx);
}

.comic-wrapper .comic-details .content {
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
}

.comic-wrapper .content .title {
  font-size: 40rpx;
  font-weight: bold;
}

.comic-wrapper .content .info {
  padding-bottom: 20rpx;
  font-size: 22rpx;
  color: var(--comic-details-font-color, rgb(150, 151, 153));
}

.comic-wrapper .info .comic-tip,
.comic-wrapper .info .comic-heat {
  margin-bottom: 10rpx;
}

.comic-wrapper .comic-tip .copyright-type,
.comic-wrapper .comic-tip .comic-status,
.comic-wrapper .info .comic-type .item {
  display: inline-block;
}

.comic-wrapper .comic-type .item {
  height: 28rpx;
  line-height: 28rpx;
  padding: 0rpx 15rpx;
  border-radius: 30rpx;
  margin-right: 10rpx;
  border: 1rpx #e6e6e6 solid;
}

.comic-wrapper .comic-tip .copyright-type {
  padding: 0rpx 15rpx;
  border-radius: 30rpx;
  color: white;
  background-image: linear-gradient(90deg,#ff6a8d,#fe4e51);
}

.comic-wrapper .comic-tip .comic-status {
  font-size: 24rpx;
  margin-left: 15rpx;
}

.comic-wrapper .info .comic-heat {
  display: block;
  font-size: 25rpx;
}

.comic-wrapper .comic-desc {
  margin-top: 40rpx;
  color: var(--comic-details-font-color, rgb(150, 151, 153));
  font-size: 30rpx;
  text-align: justify;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.comic-wrapper .slide-role {
  white-space: nowrap;
  height: 68rpx;
  padding-left: 38rpx;
  margin: 0 -38rpx;
  margin-top: 30rpx;
  overflow-x: scroll;
}

.comic-wrapper .slide-role .item {
  display: inline-flex;
  height: 68rpx;
  margin-right: 50rpx;
}

.slide-role .item .sculpture {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}

.slide-role .introduce {
  display: flex;
  flex-flow: column nowrap;
  margin-left: 15rpx;
}

.slide-role .introduce .name {
  font-size: 25rpx;
}

.slide-role .introduce .typename {
  font-size: 22rpx;
  margin-bottom: 5rpx;
}

.slide-role .introduce .name,
.slide-role .introduce .typename {
  display: block;
  color: var( --comic-details-font-color, rgb(150, 151, 153));
}

.comuic-catalogue {
  display: flex;
  height: 100rpx;
  background-color: #00fd0d18;
  border-radius: 50rpx;
  border: 1rpx solid #4ef50c73;
  margin-top: 40rpx;
  padding: 0 40rpx;
  align-items: center;
  justify-content: space-between;
}

.comuic-catalogue .left,
.comuic-catalogue .right {
  display: flex;
  align-items: center;
}

.comuic-catalogue .left .tip {
  font-size: 32rpx;
}

.comuic-catalogue .left .update-time,
.comuic-catalogue .right .chapter-name {
  width: 230rpx;
  font-size: 25rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.comuic-catalogue .left .update-time{
  margin-left: 20rpx;
}

.comuic-catalogue .right .arrow {
  color: #d5d5d5;
  font-size: 32rpx;
  margin-left: 20rpx;
}

.comuic-catalogue .right .chapter-name {
  color: #6e6a67;
  text-align: right;
}

.comuic-catalogue .left .update-time {
  color: var( --comic-details-font-color, rgb(150, 151, 153));
}


.comic-influence {
  margin-top: 40rpx;
}

.comic-influence .head,
.comic-influence .head .theme,
.comic-influence .body .heat,
.comic-influence .body .heat .rank {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.comic-influence .head .left,
.comic-influence .head .right {
  width: 31%;
  height: 100rpx;
}

.comic-influence .head .left {
  background: url(data:image/png;base64,) center / 100% 100% no-repeat;
}

.comic-influence .head .right {
  background: url(data:image/png;base64,) center / 100% 100% no-repeat;
}

.comic-influence .head .theme {
  width: 38%;
  height: 50rpx;
  border-radius: 50rpx;
  border: 1rpx solid #2cf50454;
  justify-content: center;
}

.comic-influence .theme .txt,
.comic-influence .theme .arrow-up,
.comic-influence .theme .arrow-down {
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
}

.comic-influence .theme .txt {
  font-size: 30rpx;
}

.comic-influence .theme .arrow-up, 
.comic-influence .theme .arrow-down {
  color: #d5d5d5;
  margin-left: 10rpx;
}

.comic-influence .body .heat {
  margin-top: 20rpx;
  font-size: 26rpx;
  justify-content: flex-start;
}

.comic-influence .body .heat .rank {
  margin-left: 60rpx;
}

.comic-influence .body .linebox {
  margin-top: 43rpx;
  height: 170rpx;
  border-radius: 2rpx;
  border: 1rpx solid hsla(0,0%,100%,.25);
  padding: 4rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
}

.comic-influence .linebox .line {
  height: 86rpx;
}

.linebox .line .item {
  display: inline-flex;
  flex-flow: column nowrap;
  width: 20%;
  text-align: center;
}

.linebox .line .item text {
  display: inline-block;
}

.linebox .line .item text:first-child {
  font-size: 23rpx;
}

.linebox .line .item text:last-child {
  font-size: 20rpx;
  color: var(--comic-details-font-color, rgb(150, 151, 153));
}

.btn-area {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 25rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0rpx 35rpx;
}

.btn-area .bottom-btn {
  display: inline-flex;
  height: 100rpx;
  border-radius: 100rpx;
  background-image: linear-gradient(90deg, #b9febffb, #36f710);
  align-items: center;
  justify-content: center;
}

.btn-area .bottom-btn:first-child {
  width: 258rpx;
}

.btn-area .bottom-btn:last-child {
  width: 390rpx;
}

.btn-area .bottom-btn .tip {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}

.btn-area .bottom-btn .prefix-img {
  position: relative;
  top: -19rpx;
  height: 130rpx;
  display: inline-block;
}

.btn-area .bottom-btn .prefix-icon {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 50rpx;
  margin-right: 20rpx
}


.side-wrapper {
  width: 68%;
  font-size: 28rpx;
}

.side-wrapper .side-head {
  position: sticky;
  top: 0;
  background-color: #fff;
  z-index: 2;
  padding-top: 80rpx;
}

.side-wrapper .side-head::after,
.side-wrapper .side-body .item::after {
  display: block;
  content: " ";
  width: 100%;
  height:1px;
  margin-top: 50rpx;
  border-top: 1px solid #d9d9d9;
  color: #d9d9d9;
  transform-origin: 0 0;
  transform: scaleY(0.3);
  z-index: 2;
}

.side-wrapper .side-body
{
    padding: 0rpx 30rpx;
    padding-top: 50rpx;
}
.side-wrapper .side-head .summary,
.side-wrapper .side-head .update-time {
  padding: 0rpx 30rpx;
}

.side-wrapper .side-head .summary {
  font-size: 35rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}

.side-wrapper .side-head .update-time {
  margin-bottom: 20rpx;
  font-size: 22rpx;
  color: var(--comic-details-font-color, rgb(150, 151, 153));
}

.side-wrapper .side-head .summary,
.side-wrapper .side-head .update-time {
  display: block;
}

.side-wrapper .side-body .item {
  position: relative;
  display: block;
  /* line-height: 100rpx; */
}

.side-wrapper .side-body .item .red{
  color: red;
}

.side-wrapper .side-body .item .black{
  color: black;
}